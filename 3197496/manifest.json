{
  "name": "AWS AgentCore Chatbot",
  "author": {
    "name": "Your Company",
    "email": "support@yourcompany.com",
    "url": "https://yourcompany.com"
  },
  "defaultLocale": "en",
  "private": true,
  "location": {
    "support": {
      "ticket_sidebar": {
        "url": "assets/iframe.html",
        "flexible": true
      },
      "new_ticket_sidebar": {
        "url": "assets/iframe.html",
        "flexible": true
      }
    }
  },
  "version": "1.0.0",
  "frameworkVersion": "2.0",
  "parameters": [
    {
      "name": "awsRegion",
      "type": "text",
      "required": true,
      "secure": false,
      "default": "us-east-1"
    },
    {
      "name": "awsAccessKeyId",
      "type": "text",
      "required": true,
      "secure": true
    },
    {
      "name": "awsSecretAccessKey",
      "type": "text",
      "required": true,
      "secure": true
    },
    {
      "name": "agentId",
      "type": "text",
      "required": true,
      "secure": false
    },
    {
      "name": "agentAliasId",
      "type": "text",
      "required": true,
      "secure": false
    }
  ]
}

